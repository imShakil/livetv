{
  "name": "bdiptv",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "version:set": "node scripts/set-release-version.js",
    "mobile:sync": "bash scripts/mobile-sync.sh",
    "mobile:sync:android": "bash scripts/mobile-sync.sh android",
    "mobile:sync:ios": "bash scripts/mobile-sync.sh ios",
    "android:release:apk": "bash scripts/mobile-release-android.sh apk",
    "android:release:aab": "bash scripts/mobile-release-android.sh aab",
    "android:release:apk:skip-build": "bash scripts/mobile-release-android.sh apk --skip-build",
    "android:release:aab:skip-build": "bash scripts/mobile-release-android.sh aab --skip-build",
    "ios:ipa": "bash scripts/mobile-release-ios.sh",
    "ios:ipa:skip-build": "bash scripts/mobile-release-ios.sh --skip-build",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@capacitor-community/admob": "^7.2.0",
    "@capacitor/android": "^7.5.0",
    "@capacitor/cli": "^7.5.0",
    "@capacitor/core": "^7.5.0",
    "@capacitor/ios": "^7.5.0",
    "lucide-react": "^0.552.0",
    "next": "^16.1.6",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-player": "^2.16.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "eslint": "^9.39.3",
    "eslint-config-next": "^0.2.4",
    "patch-package": "^8.0.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17"
  }
